    socket cnd stream 660 root inet

service dpmd /system/vendor/bin/dpmd
    class late_start
    socket dpmd stream 660 root system
    seclabel u:r:dpmd:s0
    group system readproc

service loc_launcher /system/vendor/bin/loc_launcher
