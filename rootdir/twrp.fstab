# mount point    fstype    device            [device2]   fstype2
/boot           emmc    /dev/block/bootdevice/by-name/boot
/recovery       emmc    /dev/block/bootdevice/by-name/FOTAKernel        flags=backup=1
/system         ext4    /dev/block/bootdevice/by-name/system
/cache          ext4    /dev/block/bootdevice/by-name/cache
/data           ext4    /dev/block/bootdevice/by-name/userdata          flags=encryptable=footer
/firmware       vfat    /dev/block/bootdevice/by-name/modem             flags=mounttodecrypt

/external_sd    vfat    /devices/soc.0/7864900.sdhci/mmc_host/mmc*      flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg        vfat    /devices/platform/msm_hsusb_host/usb*           flags=display="USB OTG";storage;wipeingui;removable
