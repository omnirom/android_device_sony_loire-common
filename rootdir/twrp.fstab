# mount point    fstype    device            [device2]   fstype2
/boot           emmc    /dev/block/bootdevice/by-name/boot
/system         ext4    /dev/block/bootdevice/by-name/system
/cache          ext4    /dev/block/bootdevice/by-name/cache
/data           ext4    /dev/block/bootdevice/by-name/userdata                         length=-16384

/external_sd    vfat    /devices/soc.0/7864900.sdhci/mmc_host/mmc*                     flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg        vfat    /devices/soc.0/*/xhci-hcd.0.auto                               flags=display="USB OTG";storage;wipeingui;removable
